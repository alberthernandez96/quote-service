{
  "name": "@albertoficial/quote-service",
  "version": "1.0.0",
  "description": "Quote microservice",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "dev:up": "docker compose -f docker-compose.dev.yml up -d && npm run start",
    "dev:down": "docker compose -f docker-compose.dev.yml down",
    "start": "node dist/index.js" 
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "zod": "^3.23.8",
    "@albertoficial/api-contracts": "1.0.5",
    "@albertoficial/backend-shared": "1.0.1",
    "@albertoficial/postgres-shared": "1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.3.3"
  }
}